alias aliases="cat $HOME/.config/env/aliases | cut -c 6- | less"
alias astyle="astyle -t8 -xV -xt -xg -H -k3 -W3 -xb -n -z2"
alias cal="cal -ymw"
alias calc="calc -p"
alias clea="clear"
alias cleanup='doas pacman -Rns $(pacman -Qtdq)'
alias cls="clear; ls"
alias cp="cp -i"
alias date="date +%F"
alias df="df -h"
alias dff="df -h | head -n 1 && df | grep --color=never '% /$'"
alias dir="LC_COLLATE=C ls -AvhlFi --color=auto"
alias du="du -sh"
alias fix-gpg-check="gpg2 --keyserver-options auto-key-retrieve --verify"
alias fix-gpg-retrieve="gpg2 --keyserver-options auto-key-retrieve --receive-keys"
alias fix-key="[ -d ~/.gnupg ] || mkdir ~/.gnupg ; cp /etc/pacman.d/gnupg/gpg.conf ~/.gnupg/ ; echo 'done'"
alias free="free -h"
alias fsurf="$HOME/.config/newsboat/surf.sh"
alias git-compress="git repack -AdflF"
alias gpg-check="gpg2 --keyserver-options auto-key-retrieve --verify"
alias gpg-retrieve="gpg2 --keyserver-options auto-key-retrieve --receive-keys"
alias gpl="cp /usr/share/licenses/common/GPL3/license.txt LICENSE"
alias hw="hwinfo --short"
alias is_defined_h="cat /home/zocki/.config/env/is_defined"
alias jctl="journalctl -p 3 -xb"
alias keep_wallpaper="touch /home/zocki/.wallpaper_change"
alias lrs="hunspell -d de_DE"
alias ls="LC_COLLATE=C ls -AvF --color=auto"
alias mail="neomutt"
alias makefile="cp /home/zocki/.config/Makefile ."
alias microcode='grep . /sys/devices/system/cpu/vulnerabilities/*'
alias mirror="doas reflector -f 30 -l 30 --number 20 --protocol https,rsync --country Germany --verbose --save /etc/pacman.d/mirrorlist"
alias mutt="neomutt"
alias nnn="nnn -Hdi"
alias onekod="daemonize $(which oneko)"
alias probe="doas -E hw-probe -all -upload"
alias rls="ls"
alias sct_cycle="sct_off && sct_on"
alias sct_off="/usr/bin/sct"
alias sct_on="/usr/bin/sct 6000 0.85"
alias sdp="spd-say -w -r 25"
alias search="pacman -Ss"
alias sl="ls"
alias spell="hunspell -d en_GB-large"
alias surf="surf -u '$SURF_USERAGENT'"
alias tree="tree -a -I ".git" -sh --du -D --timefmt "%F""
alias tty-clock="tty-clock -cs -C 1 -f %F"
alias update="doas pacman -Syu"
alias update-fc='doas fc-cache -fv'
alias update-grub="doas grub-mkconfig -o /boot/grub/grub.cfg"
alias updates="doas pacman -Syu"
alias userlist="cut -d: -f1 /etc/passwd"
alias valgrind="valgrind --leak-check=full --show-leak-kinds=all -s --track-origins=yes"
alias wallpaperlog="less -N --mouse $HOME/.config/wallpaperlog"
alias wallpapermap="less --mouse $HOME/.wallpapermap"
alias weechat="LD_PRELOAD=/home/zocki/.local/lib/libwcwidth.so weechat"
alias wmlist="/usr/bin/ls /usr/share/xsessions"
alias wttr="wttr.sh"
alias yay="yay --nocombinedupgrade"
alias zshrc="source $HOME/.zshrc"
